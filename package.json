    "next-themes": "^0.4.6",
    "react": "^19.0.0",
    "react-dom": "^19.0.0",
    "react-hook-form": "^7.56.3",
    "shadcn-dropzone": "^0.2.1",
    "sonner": "^2.0.3",
    "stripe": "^18.1.0",
    "tailwind-merge": "^3.2.0",
    "uuid": "^11.1.0",
    "zod": "^3.24.2"

    "@radix-ui/react-slot": "^1.2.2",
    "@radix-ui/react-tabs": "^1.1.11",
    "@t3-oss/env-nextjs": "^0.12.0",
    "bcryptjs": "^3.0.2",
    "class-variance-authority": "^0.7.1",
